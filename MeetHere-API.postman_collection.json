			}
		},
		{
			"name": "좌표→주소 변환 (Reverse Geocoding)",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"lat\": 37.5665,\n  \"lng\": 126.9780\n}"
				},
				"url": {
					"raw": "{{base_url}}/api/v1/naver/reverse-geocode",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"v1",
						"naver",
						"reverse-geocode"
					]
				},
				"description": "위도/경도 좌표를 한국 주소로 변환합니다."
			}
		},
		{
			"name": "네이버 API 상태 확인",
			"request": {
				"method": "GET",
				"header": [],
