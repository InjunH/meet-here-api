	"item": [
		{
			"name": "Health Check",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/api/v1/health",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"v1",
						"health"
					]
				},
				"description": "서버 상태 확인"
			}
		},
		{
			"name": "주소 자동완성",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"query\": \"서울역\",\n  \"limit\": 5\n}"
				},
				"url": {
					"raw": "{{base_url}}/api/v1/naver/autocomplete",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"v1",
						"naver",
						"autocomplete"
					]
				},
